    "@nestjs/graphql": "^13.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.7",
    "@nestjs/throttler": "^6.3.1",
    "@nestjs/terminus": "^10.2.0",
    "@nestjs/typeorm": "^10.0.2",
    "apollo-server-express": "^3.13.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.12.4",

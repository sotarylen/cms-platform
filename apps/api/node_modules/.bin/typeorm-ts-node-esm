#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sotary/dev/cms-platform/node_modules/.pnpm/typeorm@0.3.27_ioredis@5.8.2_mysql2@3.15.3_pg@8.16.3_reflect-metadata@0.2.2_ts-node@10.9.2_@t_u33bhrgzbv3us4ilcjp67vciq4/node_modules/typeorm/node_modules:/Users/sotary/dev/cms-platform/node_modules/.pnpm/typeorm@0.3.27_ioredis@5.8.2_mysql2@3.15.3_pg@8.16.3_reflect-metadata@0.2.2_ts-node@10.9.2_@t_u33bhrgzbv3us4ilcjp67vciq4/node_modules:/Users/sotary/dev/cms-platform/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sotary/dev/cms-platform/node_modules/.pnpm/typeorm@0.3.27_ioredis@5.8.2_mysql2@3.15.3_pg@8.16.3_reflect-metadata@0.2.2_ts-node@10.9.2_@t_u33bhrgzbv3us4ilcjp67vciq4/node_modules/typeorm/node_modules:/Users/sotary/dev/cms-platform/node_modules/.pnpm/typeorm@0.3.27_ioredis@5.8.2_mysql2@3.15.3_pg@8.16.3_reflect-metadata@0.2.2_ts-node@10.9.2_@t_u33bhrgzbv3us4ilcjp67vciq4/node_modules:/Users/sotary/dev/cms-platform/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
fi
